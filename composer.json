{
  "name": "sule/bank-statements",
  "description": "Laravel Bank Statements package",
  "keywords": ["bank statements", "bank"],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^5.5.9 || ^7.0",
    "illuminate/config": "5.4.* || 5.5.* || 5.6.*",
    "illuminate/filesystem": "5.4.* || 5.5.* || 5.6.*",
    "illuminate/console": "5.4.* || 5.5.* || 5.6.*",
    "illuminate/database": "5.4.* || 5.5.* || 5.6.*",
    "illuminate/support": "5.4.* || 5.5.* || 5.6.*",
    "nesbot/carbon": "^1.0",
    "guzzlehttp/guzzle": "~6.2.2",
    "ramsey/uuid": "^3.5.2"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~4.0",
    "phpspec/phpspec": "~2.1",
    "orchestra/testbench": "~3.0"
  },
  "autoload": {
    "classmap": [
      "src/Exceptions.php"
    ], 
    "psr-4": {
      "Sule\\BankStatements\\": "src"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
